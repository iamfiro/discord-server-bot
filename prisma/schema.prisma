generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UserWarn {
  userId String @unique
  resons String
  createdAt DateTime @default(now())
}

model UserLevel {
  userId String @unique
  xp BigInt @default(0)
}